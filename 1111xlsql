cd /etc/sysconfig/;
rm -rf i18n 
echo 'LANG="en_US.UTF-8" SYSFONT="latarcyrheb-sun16"'>/etc/sysconfig/i18n 
source /etc/sysconfig/i18n
source /etc/openvpn/peizhi.cfg
mkdir /home/backups/ >/dev/null 2>&1
cd /home/backups/
mysqldump -h${localhost} -u${root} -p${mima} ov >ov.sql
if [ ! -f "/home/backups/ov.sql" ]; then
	read
else
	mysql -h$localhost -u$root -p$mima ov -e "drop table line" >/dev/null 2>&1
	mysql -h$localhost -u$root -p$mima ov -e "drop table xbml_article" >/dev/null 2>&1
fi
cd /etc/openvpn/
wget https://download.lyiqk.cn/ML/xbml/2017linesql.zip >/dev/null 2>&1 
unzip 2017linesql.zip >/dev/null 2>&1
rm -rf 2017linesql.zip >/dev/null 2>&1
cacrt=`cat /etc/openvpn/easy-rsa/keys/ca.crt`
takey=`cat /etc/openvpn/easy-rsa/ta.key`
sed -i 's/【ip】/'${IP}'/g' /etc/openvpn/line.sql >/dev/null 2>&1
mysql -h$localhost -u$root -p$mima ov < /etc/openvpn/line.sql
mysql -h$localhost -u$root -p$mima ov -e "UPDATE line SET content = REPLACE( content,'【证书】','${cacrt}');" >/dev/null 2>&1
mysql -h$localhost -u$root -p$mima ov -e "UPDATE line SET content = REPLACE( content,'【key】','${takey}');" >/dev/null 2>&1
sed -i 's/【ip】/'$IP'/g' /etc/openvpn/xbml_article.sql >/dev/null 2>&1
mysql -h$localhost -u$root -p$mima ov < /etc/openvpn/xbml_article.sql
mysql -h$localhost -u$root -p$mima ov -e "UPDATE xbml_article SET content = REPLACE( content,'【证书】','${cacrt}');" >/dev/null 2>&1
mysql -h$localhost -u$root -p$mima ov -e "UPDATE xbml_article SET content = REPLACE( content,'【key】','${takey}');" >/dev/null 2>&1
rm -rf /etc/openvpn/line.sql >/dev/null 2>&1
rm -rf /etc/openvpn/xbml_article.sql >/dev/null 2>&1
cd /home/
source /etc/openvpn/peizhi.cfg
